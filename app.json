{
  "expo": {
    "name": "MumMoney",
    "slug": "mum-money",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "scheme": "mum-money",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FEFAE0"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mummoney.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FEFAE0"
      },
      "package": "com.mummoney.app",
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/c5882652-e760-4115-ad1f-8032d2f66fd3"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-secure-store",
      "expo-updates",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#2D6A4F"
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c5882652-e760-4115-ad1f-8032d2f66fd3"
      }
    },
    "owner": "aarav1656"
  }
}
